{{#each this}}
<div>
    <h2></h2><img src='{{flags.svg}}' alt='Flag: {{name.official}}' width="100px" />
        {{name.official}}
    </h2>
    <ul>
        <li>Capital: {{capital}}</li>
        <li>Population: {{population}}</li>
        <li>Languages:  {{#each languages}}
                           {{this}}{{#unless @iso639_2}},{{/unless}}
                        {{/each}} </li>
    </ul>
</div>
{{/each}}
